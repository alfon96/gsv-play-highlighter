{
    "scopeName": "source.gsv",
    "patterns": [
      {
        "name": "keyword.control.gsv",
        "match": "\\b(UNH|STX|RCI|ATC|UNT)\\b"
      },
      {
        "name": "entity.name.message",
        "match": "(?<=\\+)[A-Z]{5}(?=[:'])"
      },
      {
        "name": "punctuation.separator.semicolon",
        "match": ";"
      }
    ],
    "repository": {},
    "name": "gsv"
  }
  